<?xml version="1.0" encoding="UTF-8" ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
targetNamespace = "http://www.electionsurvey.com"
xmlns="http://www.electionsurvey.com"
elementFormDefault="qualified">
    <xs:element name="VoterDetails">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="VoterName" type="xs:string"/>
                <xs:element name="Gender" type="xs:string"/>
                <xs:element name="Age" type="xs:integer"/>
                <xs:element name="DateOfBirth" type="xs:date"/>
                <xs:element name="Profession" type="xs:string"/>
                <xs:element name="PhoneNumber" type="xs:long"/>
                <xs:element name="Address" type="xs:string"/>
                <xs:element name="City" type="xs:string"/>
                <xs:element name="State" type="xs:string"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>