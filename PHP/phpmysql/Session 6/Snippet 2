<?php
//Declaring variables and assigning values
$int_value1= 13;
$int_value2 = 45;
//Displaying the text
echo "The product of $int_value1 * $int_value2 is: ";
//Calculating the product
$int_value2=$int_value1 * $int_value2;
//Display the result
echo $int_value2;
?>
