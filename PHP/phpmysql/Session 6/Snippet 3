<?php
//Declaring and assigning values to variables
$float_value1 = 123.57;
$float_value2 = 12.87;
//Displaying the text
echo "The subtraction value of $float_value1 - $float_value2 is: ";
//Calculating the difference
$float_value2 = $float_value1 - $float_value2;
echo $float_value2;
?>
